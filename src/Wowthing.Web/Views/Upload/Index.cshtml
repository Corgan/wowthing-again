@{
    ViewData["Title"] = "Upload Addon Data";
}

<form method="post" enctype="multipart/form-data" asp-controller="Upload" asp-action="FromWeb">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload one files using this form:</p>
            <input type="file" name="luaFile" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>
